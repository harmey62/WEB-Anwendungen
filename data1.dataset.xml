<data>
	<dataset>
		<name>Data</name>
		<table>
			<name>Data1</name>
			<type>context</type>
        	<context>
				<parameter>
					<name>Condition</name>
					<type>condition</type>
					<source>
						<table>default</table>
						<column>Value</column>
					</source>
				</parameter>
        	    <maxresults>0</maxresults>
        		<basequery>HMSoftware.Entities.HMCRM.Address</basequery>
        		<basecondition>(InUse &lt; 90 AND ClientNumber = $(clientnumber) AND Gruppe &lt;  19)</basecondition>
        		<fields>
        			<field>
        				<name>Id</name>
        			</field>
        			<field>
        				<name>Id AS AddressId</name>
        			</field>
        			<field>
        				<name>('e9d258b9eadb45aeb718b6e7f0bff111') AS DataGroup</name>
        			</field>
        			<field>
        				<name>Nummer</name>
        			</field>
        			<field>
        				<name>Kurzname</name>
        			</field>
        			<field>
        				<name>Name01</name>
        			</field>
        			<field>
        				<name>Strasse</name>
        			</field>
        			<field>
        				<name>PLZ</name>
        			</field>
					<field>
        				<name>Ort</name>
        			</field>

        			<field>
        				<name>COALESCE(Anrede, '') || ' ' || COALESCE(Titel, '') || ' ' || COALESCE(VorName, '') || ' ' || COALESCE(Name, '') AS Ansprechpartner</name>
        			</field>
        			<field>
        				<name>EMail</name>
        			</field>
        			<field>
        				<name>Telefon</name>
        			</field>
        			<field>
        				<name>ClientNumber</name>
        			</field>
        			<field>
        				<name>Gruppe</name>
        			</field>
        			<field>
        				<name>InUse</name>
        			</field>
        			<field>
        				<name>Funktelefon AS Mobil</name>
        			</field>
        			<field>
        				<name>0 AS Type</name>
        			</field>
					<field>
        				<name>Birthday</name>
        			</field>
        			<field>
        				<name>(0) AS _DetailsCount</name>
        			</field>
        		</fields>
        		<addcolumns>
        			<name>Karte</name>
        			<query>HMSoftware.Entities.HMCORE.GeoCoordinates</query>
        			<basefield>Nummer</basefield>
        			<tablefield>MasterId</tablefield>
        			<fields>
        				<field>
        					<name>(1)</name>
        					<alternativename>_Map</alternativename>
        				</field>
        			</fields>
        		</addcolumns>
        		<addcolumns>
        			<caption>Details</caption>
        			<query>HMSoftware.Entities.HMCRM.AddressContact</query>
        			<basefield>Nummer</basefield>
        			<tablefield>AddressNumber</tablefield>
        			<fields>
        				<field>
        					<name>COUNT(Id) AS Anzahl</name>
        					<alternativename>_DetailsCount</alternativename>
        				</field>
        			</fields>
        		</addcolumns>
        	</context>
		</table>
	</dataset>
</data>

